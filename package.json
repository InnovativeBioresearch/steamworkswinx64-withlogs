{
    "name": "steamworkswinx64-withlogs",
    "version": "0.0.1",
    "main": "index.js",
    "types": "index.d.ts",
    "description": "Steamworks.js with additional logging for Windows x64",
    "author": "Jonathan Fior -- Innovative Bioresearch",
    "license": "MIT",
    "napi": {
        "name": "steamworksjs",
        "triples": {
            "additional": ["x86_64-pc-windows-msvc"]
        }
    },
    "files": [
        "dist/win64/steam_api64.dll",
        "dist/win64/steam_api64.lib",
        "dist/win64/steamworksjs.win32-x64-msvc.node",
        "sdk/redistributable_bin/win64/steam_api64.dll",
        "sdk/redistributable_bin/win64/steam_api64.lib",
        "index.js",
        "index.d.ts",
        "client.d.ts",
        "callbacks.d.ts",
        "Cargo.toml",
        "Cargo.lock",
        "build.rs",
        "build.js",
        "src/lib.rs",
        "src/client.rs",
        "README.md"
    ],
    "dependencies": {
        "@types/node": "^20.14.0"
    },
    "devDependencies": {
        "@napi-rs/cli": "2.18.4",
        "electron": "24.2.0",
        "rimraf": "6.0.1",
        "typescript": "5.5.3"
    },
    "engines": {
        "node": ">= 14"
    },
    "scripts": {
        "build": "node build.js x86_64-pc-windows-msvc",
        "build:debug": "node build.js x86_64-pc-windows-msvc --debug",
        "prune": "rimraf dist target client.d.ts",
        "types": "tsc index.js --allowJs --declaration --emitDeclarationOnly",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/InnovativeBioresearch/steamworkswinx64-withlogs"
    },
    "keywords": ["steamworks", "electron", "steam", "logging", "windows"]
}